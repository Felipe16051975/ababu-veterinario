{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images|lib)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "installCommand": "echo 'Dependencies already included'",
  "buildCommand": "echo 'Build not needed'"
}